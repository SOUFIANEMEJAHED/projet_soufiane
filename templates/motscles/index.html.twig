{% extends 'base.html.twig' %}

{% block title %}Motscles index{% endblock %}

{% block body %}
    <h1>Motscles index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>Motcle</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for motscle in motscles %}
            <tr>
                <td>{{ motscle.id }}</td>
                <td>{{ motscle.motcle }}</td>
                <td>
                    <a href="{{ path('app_motscles_show', {'id': motscle.id}) }}">show</a>
                    <a href="{{ path('app_motscles_edit', {'id': motscle.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="3">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('app_motscles_new') }}">Create new</a>
{% endblock %}
